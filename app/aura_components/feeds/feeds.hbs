<link href="./bower_components/paper-item/paper-item.html" rel="import">
<% feeds.forEach(function(model) { %>
<a class="feed" href="#<%= model.id %>">
    <paper-item class="hover" layout start>
        <img class="fav"
             src="http://favicon.yandex.ru/favicon/<%= new URL(model.get('link')).hostname %>" self-start>
        <div self-start>
            <%= model.get('title') %>
        </div>
        <button type="button" class="close" data-id="<%= model.id %>" self-start><span aria-hidden="true">Ã—</span><span
                class="sr-only">Close</span></button>
    </paper-item>
</a>
<% }); %>