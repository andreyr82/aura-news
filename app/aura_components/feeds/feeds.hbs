<link href="./bower_components/paper-item/paper-item.html" rel="import">
<% feeds.forEach(function(model) { %>

    <paper-item>
        <a class="feed" href="#<%= model.id %>" layout horizontal>
        <img class="fav"
             src="http://favicon.yandex.ru/favicon/<%= new URL(model.get('link')).hostname %>">
        <%= model.get('title') %>
        <button type="button" class="close" data-id="<%= model.id %>"><span aria-hidden="true">Ã—</span><span
                class="sr-only">Close</span></button>
        </a>
    </paper-item>
<% }); %>